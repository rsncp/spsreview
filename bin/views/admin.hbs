


 <section>
        <div class="container">
            
            <h1 class="heading">SPS Alumni</h1>
            <button type="button"  onclick="location.href='formdisplay'" class="btn btn-info">+ New Candidate</button>
            <br><br>


            <div class="card-wrapper">

               
            {{#each datas}}

                <div class="card">
                    <div class="textoverimg">
                        <img src="../images/spss.jpg" alt="card background" class="card-img">
                        <div class="text-block">
                            <i class="fa fa-edit" style="font-size: 2em;padding-right: 9px;"></i>

                            <i class="fa fa-trash"  style="font-size: 2em;"></i>
                        </div>
                    </div>



                    <img src="{{this.uploadfile}}" alt="profile image" class="profile-img">
                    <h1>{{this.name}}</h1>
                    <p class="job title">sps holder</p>
                    <p class="about">Name: {{this.name}}<br>
                        place : {{this.place}}<br>
                        Gender : {{this.gender}} <br>
                        email : {{this.email}}</p>
                    <a href="Resume-CV-Design-N19-master/index.html" class="btn">more info</a>
                    <ul class="social-media">
                        <li><a href="{{this.facebook}}"><i class="fa fa-facebook-square"></i></a></li>
                        <li><a href="https://t.me/{{this.telegram}}"><i class="fa fa-telegram"></i></a></li>
                        <li><a href="{{this.linkedin}}"><i class="fa fa-linkedin-square"></i></a></li>
                        <li><a href="https://api.whatsapp.com/send?phone={{this.whatsapp}}"><i class="fa fa-whatsapp"></i></a></li>
                    </ul>
                </div>

            {{/each}}


            </div>
        </div>
    </section>